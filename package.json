{
  "name": "heroku-cra-node",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd react-ui/ && npm install --only=dev && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "express-session": "^1.15.2",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.2",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-twitter": "^1.0.4",
    "request": "^2.81.0"
  },
  "keywords": [
    "node",
    "heroku",
    "react",
    "mongodb",
    "express"
  ],
  "license": "MIT",
  "devDependencies": {}
}
